{
  "name": "nuxt-swiper",
  "version": "1.1.0",
  "keywords": [
    "vue",
    "nuxt",
    "nuxt3",
    "nuxt-module",
    "swiper",
    "swiperjs",
    "slides",
    "slickjs alternative"
  ],
  "license": "MIT",
  "type": "module",
  "repository": "cpreston321/nuxt-swiper",
  "homepage": "https://github.com/cpreston321/nuxt-swiper#readme",
  "author": {
    "name": "cpreston321 <https://christianpreston.com>",
    "url": "https://github.com/cpreston321"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint --ext .ts,.vue .",
    "lint:fix": "eslint --fix --ext .ts,.vue .",
    "release": "pnpm run lint && pnpm run test && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.3",
    "swiper": "^9.4.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/test-utils": "^3.5.3",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@typescript-eslint/parser": "^5.59.11",
    "changelogen": "^0.5.3",
    "eslint": "^8.43.0",
    "nuxt": "^3.5.3",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "packageManager": "pnpm@8.5.1"
}
